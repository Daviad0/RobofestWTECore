            function Countdown() {
                if (timer == true) {
                    if (secondsc == 0) {
                        if (minutesc > 0) {
                            minutesc = minutesc - 1;
                            secondsc = 59;
                            secondsview = "59";
                        } else {
                            timer = false;
                            TimerEnded();
                        }

                    } else if (secondsc < 11) {
                        secondsc = secondsc - 1;
                        secondsview = "0" + secondsc;
                    }
                    else {
                        secondsc = secondsc - 1;
                        secondsview = secondsc;
                    }
                }
            }
            function StartTimer() {
                var count = setInterval(function () {

                    if (abort == true) {
                        clearInterval(count);
                        chat.invoke("sendTimer",minutesc, secondsc, message, 2);
                    } else {
                        chat.invoke("sendTimer",minutesc, secondsc, message, 1);
                    }

                    if ((part % 10) == 0) {
                        Countdown();
                    }
                    part++;
                    window.focus();
                }, 100);
            }
